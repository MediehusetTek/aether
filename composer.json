{
    "name": "tumedia/aether",
    "description": "Aether - A modular PHP framework",
    "keywords": ["aether"],
    "license": "MIT",
    "require": {
        "php": ">=5.4",
        "smarty/smarty": "3.1.21",
        "illuminate/view": "5.3.*",
        "illuminate/filesystem": "5.3.*",
        "illuminate/events": "5.3.*",
        "illuminate/container": "5.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5"
    },
    "autoload": {
        "classmap": [
            "./src"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "./tests"
        ]
    },
    "bin": [
        "./bin/generate-config",
        "./bin/view-clear"
    ]
}
